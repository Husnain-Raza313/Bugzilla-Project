
<div class="m-3">
<table class="table table-hover table-striped table-bordered">
  <thead >
    <tr class="table-dark">
      <th scope="col">User ID</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
      <th scope="col">User Type</th>
      <th scope="col"></th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <% @users.each do |user| %>
    <tbody>
    <tr>
      <th scope="row"><%= user.id%></th>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= user.user_type.capitalize %></td>
      <td><%= link_to "Show ", users_show_path(user), class:"btn btn-primary" %></td>
      <td><%= link_to "Show Assigned Projects", user_projects_show_path(user), class:"btn btn-warning" %></td>
      <td><%= link_to "Show Unassigned Projects", user_projects_unassigned_show_path(user), class:"btn btn-success" %></td>



    </tr>
  </tbody>

  <% end %>
  </table>

  <%# <%= link_to "New Project", new_project_path, class:'btn btn-secondary'%>

</div>

<h1 class='m-3'>Assigned Projects </h1>

<div class="m-3">
<table class="table table-hover table-striped table-bordered">
  <thead >
    <tr class="table-dark">
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Created At</th>
      <th scope="col">Updated At</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <% @projects.each do |devproject| %>
    <%# <%= render friend %>
    <tbody>
    <tr>
      <th scope="row"><%= devproject.project_id%></th>
      <td><%= devproject.name %></td>
      <td><%= devproject.created_at %></td>
      <td><%= devproject.updated_at %></td>
      <td><%= link_to "Show Bugs List", project_path(devproject), class:"btn btn-primary" %></td>







    </tr>
  </tbody>

  <% end %>
  </table>


  </div>

